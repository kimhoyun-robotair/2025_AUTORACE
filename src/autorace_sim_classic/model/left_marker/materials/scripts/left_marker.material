material Leftmarker/Albedo
{
  receive_shadows on
  technique
  {
    pass
    {
      lighting on
      depth_write on
      depth_check on

      ambient 1 1 1 1
      diffuse 1 1 1 1
      specular 0 0 0 1 8

      texture_unit
      {
        texture left.png
        tex_address_mode wrap
        filtering trilinear
      }
    }
  }
}

